.map-page {
  padding: 20rpx;
}
.map-page .search {
  width: 100%;
  position: relative;
}
.map-page .search input {
  width: 85%;
  padding: 10rpx 50rpx 10rpx 60rpx;
  background: var(--themeBg);
}
.map-page .search .iconfont {
  font-size: var(--iconSize);
  position: absolute;
  top: 50%;
  color: var(--lightFontColor);
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
}
.map-page .search .iconfont.icon-qingchu {
  right: 20rpx;
  font-size: 36rpx;
  color: #cccccc;
  z-index: 2;
}
.map-page .search .iconfont.icon-search {
  left: 15rpx;
}
.map-page .search .scroll {
  margin: 20rpx 0;
  max-height: 500rpx;
  position: absolute;
  z-index: 3;
  background: #fff;
  top: 50rpx;
  left: 0;
}
.map-page .search .scroll .scroll-item {
  color: var(--lightFontColor);
  padding: 8rpx;
}
.map-page .map-container {
  width: 100%;
  margin-top: 80rpx;
}
.map-page .map-container .title {
  color: var(--lightFontColor);
}
.map-page .map-container .map {
  margin-top: 10rpx;
  width: 100%;
}