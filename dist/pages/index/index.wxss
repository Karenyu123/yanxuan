.home .header {
  position: relative;
  width: 100%;
  padding: 0 20rpx;
  align-items: center;
  display: flex;
}
.home .header .location {
  width: 20%;
  text-align: center;
  color: var(--darkFontColor);
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  margin-right: 10rpx;
  font-size: 24rpx;
}
.home .header .location .icon-weizhi {
  font-size: 30rpx;
  color: #4196F9;
}
.home .header .search {
  width: 60%;
  background: #f1f1f1;
  height: 40rpx;
  padding: 10rpx 40rpx 10rpx 60rpx;
  font-size: 26rpx;
}
.home .header .icon-search {
  position: absolute;
  font-size: var(--iconSize);
  left: 24%;
  color: var(--lightFontColor);
}
.home .swiper {
  width: 100vw;
  min-height: 360rpx;
  text-align: center;
  margin-top: 30rpx;
}
.home .swiper .swiper-item {
  width: 100%;
}
.home .swiper .swiper-item .image {
  width: 90%;
  border-radius: 20rpx;
}
.home .channel {
  padding: 20rpx;
  margin-top: 20rpx;
  display: flex;
  width: 100%;
  box-sizing: border-box;
  justify-content: space-around;
}
.home .channel .channel-item {
  flex: 1;
  text-align: center;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.home .channel .channel-item .img {
  width: 60rpx;
  margin-bottom: 6rpx;
}
.home .channel .channel-item .text {
  color: var(--lightFontColor);
  font-size: 24rpx;
}
.home .brand-list {
  overflow: hidden;
}
.home .brand-list .title {
  text-align: center;
  color: #666666;
  padding: 40rpx;
}
.home .brand-list .list-item {
  width: 46%;
  float: left;
  position: relative;
  margin-left: 20rpx;
}
.home .brand-list .list-item .item-info {
  position: absolute;
  left: 20rpx;
  top: 0;
  z-index: 1;
  display: flex;
  flex-direction: column;
  color: var(--lightFontColor);
  font-size: 24rpx;
}
.home .brand-list .list-item .item-info .price {
  color: var(--themeColor);
  font-weight: 700;
}
.home .brand-list .list-item .img {
  width: 100%;
  max-height: 214rpx;
}
.home .new-goods {
  width: 100%;
  margin-top: 40rpx;
}
.home .new-goods .new-header {
  width: 750rpx;
  height: 300rpx;
  background-image: url("data:image/png;base64,");
  background-size: contain;
  text-align: center;
  color: #96A3B0;
  overflow: hidden;
}
.home .new-goods .new-header .first {
  display: block;
  font-size: 40rpx;
  font-weight: 700;
  margin-top: 80rpx;
  margin-bottom: 20rpx;
}
.home .new-goods .new-header .all {
  display: inline-block;
  width: 120rpx;
  height: 40rpx;
  padding: 10rpx 20rpx;
  line-height: 40rpx;
  background: #D2E0EF;
  font-size: 26rpx;
}
.home .new-goods .goods-list {
  width: 100%;
  white-space: nowrap;
}
.home .new-goods .goods-list .list-item {
  display: inline-block;
  width: 280rpx;
  height: 416rpx;
  text-align: center;
  box-sizing: border-box;
}
.home .new-goods .goods-list .list-item .img {
  width: 90%;
}
.home .new-goods .goods-list .list-item .name {
  padding: 0 40rpx;
  box-sizing: border-box;
  font-weight: 700;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  color: #666666;
}
.home .new-goods .goods-list .list-item .desc {
  padding: 10rpx 20rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  color: var(--lightFontColor);
  font-size: 24rpx;
}
.home .new-goods .goods-list .list-item .price {
  text-align: left;
  padding: 0 40rpx;
  color: var(--themeColor);
  font-weight: 700;
}
.home .hot-goods .new-header {
  background-image: url("data:image/png;base64,");
}
.home .hot-goods .new-header .first {
  color: #D0C5A3;
}
.home .hot-goods .new-header .all {
  color: #D0C5A3;
  background: #F3E6C5;
}
.home .topic-list {
  width: 100%;
  padding-bottom: 40rpx;
}
.home .topic-list .title {
  text-align: center;
  color: var(--lightFontColor);
  padding: 40rpx;
}
.home .topic-list .title .iconfont {
  font-size: 30rpx;
  padding-left: 20rpx;
}
.home .topic-list .scroll {
  white-space: nowrap;
}
.home .topic-list .scroll .list-item {
  display: inline-block;
  width: 500rpx;
  margin-left: 20rpx;
}
.home .topic-list .scroll .list-item:last-child {
  margin-right: 20rpx;
}
.home .topic-list .scroll .list-item .img {
  width: 100%;
  max-height: 276rpx;
  border-radius: 18rpx;
}
.home .topic-list .scroll .list-item .name {
  display: flex;
  justify-content: space-between;
  font-weight: 700;
  color: #666666;
  font-size: 26rpx;
  margin: 10rpx 0;
}
.home .topic-list .scroll .list-item .name .price {
  color: var(--themeColor);
}
.home .topic-list .scroll .list-item .desc {
  color: var(--lightFontColor);
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  font-size: 24rpx;
}
.home .new-cate {
  margin-top: 40rpx;
}
.home .new-cate .title {
  padding: 20rpx 0;
  text-align: center;
  color: var(--lightFontColor);
  font-weight: 700;
  font-size: 32rpx;
  background: #f1f1f1;
}
.home .new-cate .cate-list {
  display: flex;
  flex-wrap: wrap;
}
.home .new-cate .cate-list .goods-item {
  width: 47%;
  margin: 10rpx 10rpx;
}
.home .new-cate .cate-list .goods-item .img {
  width: 100%;
}
.home .new-cate .cate-list .goods-item .name {
  color: var(--lightFontColor);
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  font-weight: 700;
  margin-bottom: 10rpx;
  padding: 0 30rpx;
}
.home .new-cate .cate-list .goods-item .price {
  color: var(--themeColor);
  font-weight: 700;
  padding: 0 30rpx;
}
.home .new-cate .cate-list .last {
  color: var(--lightFontColor);
  font-size: 36rpx;
  text-align: center;
  line-height: 440rpx;
}
.home .new-cate .cate-list .last .iconfont {
  font-size: 40rpx;
  margin-left: 20rpx;
  font-weight: 700;
}