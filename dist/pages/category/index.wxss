.category .header {
  position: relative;
  width: 100%;
  padding: 0 20rpx;
  align-items: center;
  display: flex;
}
.category .header .search {
  width: 80%;
  background: #f1f1f1;
  height: 40rpx;
  padding: 10rpx 40rpx 10rpx 60rpx;
  font-size: 26rpx;
}
.category .header .icon-search {
  position: absolute;
  font-size: var(--iconSize);
  left: 5%;
  color: var(--lightFontColor);
}
.category .content-wrapper {
  width: 100vw;
  box-sizing: border-box;
  overflow: hidden;
  margin-top: 30rpx;
  display: flex;
}
.category .content-wrapper .nav-left {
  width: 20%;
  text-align: center;
  line-height: 80rpx;
  color: var(--lightFontColor);
}
.category .content-wrapper .nav-left .active {
  color: var(--themeColor);
  font-weight: 700;
  border-left: 4rpx solid var(--themeColor);
}
.category .content-wrapper .category-right {
  width: 80%;
  box-sizing: border-box;
  overflow: hidden;
  padding: 0 30rpx;
}
.category .content-wrapper .category-right .img-wrapper .img {
  width: 100%;
}
.category .content-wrapper .category-right .title {
  color: var(--lightFontColor);
  font-size: 32rpx;
  text-align: center;
  margin: 20rpx 0;
}
.category .content-wrapper .category-right .list {
  display: flex;
  flex-wrap: wrap;
}
.category .content-wrapper .category-right .list .item {
  width: 33%;
  color: var(--lightFontColor);
  text-align: center;
  font-size: 26rpx;
  margin-bottom: 20rpx;
}
.category .content-wrapper .category-right .list .item .avatar {
  width: 100%;
  max-height: 120rpx;
}