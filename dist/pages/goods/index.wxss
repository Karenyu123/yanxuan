.goods-detail {
  padding-bottom: 90rpx;
  width: 100vw;
  box-sizing: border-box;
  overflow: hidden;
}
.goods-detail .swiper {
  position: relative;
  width: 100%;
  height: 750rpx;
}
.goods-detail .swiper .swiper-container {
  width: 100%;
  height: 100%;
}
.goods-detail .swiper .swiper-container .swiper-item {
  width: 100%;
  height: 100%;
}
.goods-detail .swiper .swiper-container .swiper-item .img {
  width: 100%;
  height: 100%;
}
.goods-detail .swiper .btn {
  position: absolute;
  width: 180rpx;
  height: 80rpx;
  background: var(--themeColor);
  right: 0;
  top: 100rpx;
  color: #ffffff;
  line-height: 80rpx;
  font-size: 28rpx;
  border-radius: 40rpx 0 0 40rpx;
}
.goods-detail .delivery {
  width: 100%;
  height: 80rpx;
  background: #f1f1f1;
  box-sizing: border-box;
  overflow: hidden;
  color: #666666;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 20rpx;
}
.goods-detail .info {
  width: 100%;
  height: 200rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  padding: 40rpx 0;
}
.goods-detail .info .name {
  font-size: 40rpx;
}
.goods-detail .info .desc {
  color: var(--lightFontColor);
  font-size: 28rpx;
}
.goods-detail .info .price {
  color: var(--themeColor);
  font-size: 36rpx;
}
.goods-detail .select-info {
  display: flex;
  justify-content: space-between;
  padding: 0 20rpx;
  color: var(--lightFontColor);
  height: 80rpx;
  line-height: 80rpx;
  border-top: 1rpx solid #f1f1f1;
  border-bottom: 1rpx solid #f1f1f1;
}
.goods-detail .select-info .iconfont {
  font-size: 32rpx;
}
.goods-detail .pop-section {
  width: 100%;
  box-sizing: border-box;
  position: fixed;
  padding-top: 36rpx;
  bottom: 90rpx;
  left: 0;
  z-index: 10;
  background: #fff;
  border-top: 1rpx solid #ccc;
}
.goods-detail .pop-section .mask {
  position: absolute;
  width: 100%;
  height: 100vh;
  background: rgba(0, 0, 0, 0.5);
  z-index: 8;
}
.goods-detail .pop-section .icon-cha {
  position: absolute;
  right: 10rpx;
  top: 10rpx;
  font-size: 36rpx;
}
.goods-detail .pop-section .top {
  display: flex;
  padding: 0 36rpx 20rpx;
}
.goods-detail .pop-section .top .img {
  width: 300rpx;
}
.goods-detail .pop-section .top .info {
  padding: 0;
}
.goods-detail .pop-section .top .info .name {
  font-size: 28rpx;
  color: #666666;
}
.goods-detail .pop-section .top .info .price {
  text-align: left;
  font-size: 28rpx;
}
.goods-detail .pop-section .top .info .num {
  display: flex;
  justify-content: space-between;
}
.goods-detail .pop-section .top .info .num .icon {
  display: inline-block;
  padding: 0 20rpx;
  border: 2rpx solid var(--themeColor);
  border-radius: 8rpx;
  margin: 0 20rpx;
}
.goods-detail .goods-data {
  padding: 36rpx;
}
.goods-detail .goods-data .title {
  font-size: 30rpx;
  color: #666666;
  padding-bottom: 20rpx;
  font-weight: bold;
}
.goods-detail .goods-data .data-item {
  display: flex;
  line-height: 60rpx;
}
.goods-detail .goods-data .data-item .name {
  width: 20%;
  text-align: left;
  font-size: 26rpx;
  color: var(--lightFontColor);
}
.goods-detail .goods-data .data-item .value {
  width: 80%;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
.goods-detail .footer {
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 10;
  height: 90rpx;
  width: 100%;
  display: flex;
  background: #fff;
  line-height: 90rpx;
  text-align: center;
  border-top: 1rpx solid #f1f1f1;
  color: #666666;
}
.goods-detail .footer .iconfont {
  width: 20%;
  font-size: 36rpx;
}
.goods-detail .footer .iconfont.icon-shoucang {
  color: var(--themeColor);
}
.goods-detail .footer .cart {
  width: 30%;
}
.goods-detail .footer .buy {
  flex: 1;
  background: var(--themeColor);
  color: #ffffff;
}
.goods-detail .issue-wrapper {
  padding: 36rpx 20rpx;
}
.goods-detail .issue-wrapper .title {
  text-align: center;
  font-weight: 700;
  color: #666666;
  margin-bottom: 20rpx;
  font-size: 30rpx;
}
.goods-detail .issue-wrapper .issues .issue-item .question {
  margin: 20rpx 0;
}
.goods-detail .issue-wrapper .issues .issue-item .question .dot {
  display: inline-block;
  width: 8rpx;
  height: 8rpx;
  background: var(--themeColor);
  margin-right: 10rpx;
  vertical-align: middle;
}
.goods-detail .issue-wrapper .issues .issue-item .answer {
  margin-left: 10rpx;
  color: var(--lightFontColor);
}
.goods-detail .populate {
  padding: 36rpx 20rpx;
}
.goods-detail .populate .title {
  text-align: center;
  font-size: 32rpx;
  font-weight: 700;
  color: #666666;
}
.goods-detail .populate .goods-list {
  display: flex;
  flex-wrap: wrap;
}
.goods-detail .populate .goods-list .goods-item {
  width: 47%;
  margin: 10rpx;
}
.goods-detail .populate .goods-list .goods-item .img {
  width: 100%;
}
.goods-detail .populate .goods-list .goods-item .name {
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  padding: 20rpx;
  color: var(--lightFontColor);
}
.goods-detail .populate .goods-list .goods-item .price {
  padding: 0 20rpx;
  color: var(--themeColor);
}